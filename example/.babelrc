{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "ie9": true
      },
      "uglify": true
    }],
    "react",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-es2015-modules-commonjs",
    "transform-decorators-legacy",
    "lodash",
    ["import", { 
      "libraryName": "antd", 
      "libraryDirectory": "lib",
      "style": "css" 
    }]
  ]
}